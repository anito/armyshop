{
  "server": {
    "port"  : 9294
  },
  "routes": [
    { ""        : "./public" },
    { "/test"    : "./test/public" }
  ],
  
  "packages": {
    "js": {
      "modules": [
        "es5-shimify", 
        "json2ify",
        "jquery.tmpl",
        "spine",
        "spine/lib/local",
        "spine/lib/ajax",
        "spine/lib/route",
        "spine/lib/manager",
        "spine/lib/list"
      ],
      "libs"    : [
        "lib/jade_runtime.js",
        "lib/jquery-1.9.1.js",
        "lib/jquery.tmpl.js",
        "lib/jquery-ui-1.10.3.custom.js",
        "lib/bootstrap.js"
      ],
      "paths"  : ["./app"],
      "target" : "./public/application.js",
      "jsAfter": "jade.rethrow = function rethrow(err, filename, lineno){ throw err; } "
    },
    "css": {
      "paths"  : "./css",
      "target" : "./public/application.css"
    }
  }
}
